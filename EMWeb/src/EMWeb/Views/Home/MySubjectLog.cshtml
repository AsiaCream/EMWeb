@model IEnumerable<MySubjectLog>
<h2>我的选题日志</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>题号</th>
            <th>题目名称</th>
            <th>创建时间</th>
            <th>选择导师</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <th>@x.Id</th>
                <th>@x.SNumber</th>
                <th>@x.STitle</th>
                <th>@x.Teacher</th>
                <th>@x.Time</th>
            </tr>

        }
    </tbody>
</table>
@Html.MakePager()
