@model IEnumerable<MySubjectLog>
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h3 style="margin:0;padding:0;font-family:微软雅黑;" class="text-center">我的选题日志</h3>
            <table class="table table-hover table-responsive table-striped">
                <thead>
                    <tr>
                        <th>日志ID</th>
                        <th>题目ID</th>
                        <th>题目名称</th>
                        <th>创建时间</th>
                        <th>选择导师</th>
                        <th>审核时间</th>
                        <th>审核状态</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var x in Model)
                    {
                        <tr>
                            <th>@x.Id</th>
                            <th>@x.SNumber</th>
                            <th>@x.STitle</th>
                            <th>@x.Time</th>
                            <th>@x.Teacher</th>
                            <th>@x.DrawTime</th>
                            <th>@x.Draw</th>
                        </tr>

                    }
                </tbody>
            </table>
            @Html.MakePager("page-item", "", "pager")
        </div>
    </div>
</div>
