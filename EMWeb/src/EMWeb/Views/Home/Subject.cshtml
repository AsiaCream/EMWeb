@model IEnumerable<Subject>
<div class="container">
    <h3 style="margin:0;padding:0;font-family:微软雅黑;" class="text-center">当前毕业设计选择情况</h3>
    <table class="table table-hover table-striped">
        <thead>
            <tr>
                <th>题目</th>
                <th>选择老师</th>
                <th>学生姓名</th>
                <th>提交时间</th>
                <th>审核状态</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var x in Model)
            {
                <tr>
                    <td id="td-@x.Id">@x.Title</td>
                    <td>@x.Teacher.Name</td>
                    <td>@x.Student.Name</td>
                    <td>@x.PostTime</td>
                    <td>@x.Draw</td>
                </tr>
            }
        </tbody>
    </table>
    @Html.MakePager("pager-item", "", "pager")
</div>
