@{
    Layout = null;
}
@model IEnumerable<CollegeMajor>
<div class="row">
    <div class="col-md-12">
        <table class="table table-hover table-striped">
            <tbody id="new-major-list">
                @foreach (var x in Model)
                {
                    <tr id="tr-@x.MajorNumber">
                        <td>@x.MajorNumber</td>
                        <td>@x.MajorName</td>
                        <td>@x.CollegeNumber</td>
                        <td>@x.CollegeName</td>
                        <td><a href="javascript:edit(@x.MajorNumber)" class="btn btn-sm btn-success">编辑</a><a href="javascript:void(0)" onclick="remove(@x.MajorNumber)" class="btn btn-sm btn-danger">删除</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


