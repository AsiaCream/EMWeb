@model IEnumerable<SystemLog>
<div class="container">
    <h3 style="font-family:微软雅黑;">教师日志</h3>
    <table class="table table-hover table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>操作时间</th>
                <th>管理者ID</th>
                <th>管理者姓名</th>
                <th>角色</th>
                <th>操作类型</th>
                <th>目标ID</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var x in Model)
            {
                <tr>
                    <td>@x.Id</td>
                    <td>@x.Time</td>
                    <td>@x.AdminNumber</td>
                    <td>@x.AdminName</td>
                    <td>@x.Role</td>
                    <td>@x.Operation</td>
                    <td>@x.TargetNumber</td>
                </tr>
            }
        </tbody>
    </table>
    @Html.MakePager("pager-item", "", "pager")
</div>



