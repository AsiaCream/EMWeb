@{ 
    Layout = null;
}
@model IEnumerable<College>
    <div class="row">
        <div class="col-md-12">
            <table class="table table-hover table-striped">
                <tbody id="new-college-list">
                    @foreach (var x in Model)
                    {
                        <tr id="tr-@x.Id">
                            <td>@x.Id</td>
                            <td>@x.Title</td>
                            <td><a href="javascript:edit(@x.Id)" class="btn btn-sm btn-success">编辑</a><a href="javascript:void(0)" onclick="remove(@x.Id)" class="btn btn-sm btn-danger">删除</a></td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
