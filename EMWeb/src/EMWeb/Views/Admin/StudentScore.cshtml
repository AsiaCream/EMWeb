@model IEnumerable<StudentList>
    <div class="container">
        <h3 style="margin:0;padding:0;font-family:微软雅黑;" class="text-center">学生分数列表</h3>
        <table class="table table-hover table-striped">
            <thead>
                <tr>
                    <th>编号</th>
                    <th>姓名</th>
                    <th>学号</th>
                    <th>学院</th>
                    <th>专业</th>
                    <th>题目</th>
                    <th>指导老师</th>
                    <th>评分老师</th>
                    <th>分数</th>
                    <th>评分时间</th>
                </tr>
            </thead>
            <tbody id="subject-list">
                @foreach (var x in Model)
                {
                    <tr id="table-tr-@x.Id">
                        <td>@x.Id</td>
                        <td id="name-@x.SubjectNumber">@x.Name</td>
                        <td>@x.StudentNumber</td>
                        <td>@x.College</td>
                        <td>@x.Major</td>
                        <td id="subject-@x.SubjectNumber">@x.Subject</td>
                        <td>@x.Teacher</td>
                        <td>@x.ScoreTeacher</td>
                        <td>@x.Result</td>
                        <td>@x.CreateTime</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

